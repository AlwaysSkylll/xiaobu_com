{
  "defaultSeverity": "none",
  "extends": ["tslint:latest", "tslint-react"],
  "jsRules": {},
  "rules": {
    "eofline": true,
    "no-console": false,
    "no-construct": true,
    "no-debugger": true,
    "no-reference": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "trailing-comma": false,
    "semicolon": false
  }
}
